  1 // ex1102.c  Division
  2 #include <stdio.h>
  3 short div(int x, unsigned int y)
  4 {
  5    int quotient = 0;
  6    while (1)
  7    {
  8       x = x - y;
  9       if (x < 0)
 10          break;
 11       quotient++;
 12    }   
 13    return quotient;
 14 }
 15 //==================================
 16 int main()
 17 {
 18    printf("%d\n", div(77, 7));
 19    return 0;
 20 }
